<?xml version="1.0" encoding="UTF-8"?>
<proxy name="AutosalesProxy" startOnLoad="true" transports="http https" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <endpoint key="AutosalesEndpoint"/>
        <inSequence>
            <log description="QueryParams">
                <property expression="get-property('query.param.make')" name="Make"/>
                <property expression="get-property('query.param.model')" name="Model"/>
                <property expression="get-property('query.param.version')" name="Version"/>
                <property expression="get-property('POST_TO_URI')" name="PostToUri"/>
            </log>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
        <faultSequence/>
    </target>
</proxy>
